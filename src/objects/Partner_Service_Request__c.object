    <validationRules>
        <fullName>Financial_Request_Date</fullName>
        <active>true</active>
        <errorConditionFormula>From__c   &gt;  TODAY() ||  To__c    &gt;  TODAY()</errorConditionFormula>
        <errorMessage>Date Specified should not be more than Today&apos;s Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SR_Status_Change_Restriction_CRM_User_Ac</fullName>
        <active>true</active>
        <description>This validation is being used to ensure that the CRM user cannot change the status after a Service Request has been &quot;Executed&quot;</description>
        <errorConditionFormula>AND ((OR ($Profile.Name = &quot;System Administrator&quot;, $Profile.Name = &quot;PRM User CRM&quot;)), (ISCHANGED (Status__c)), (OR (ISPICKVAL(Status__c, &apos;Open&apos;), ISPICKVAL(Status__c, &apos;Closed&apos;))))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>The Su-Kam CRM can only maintain the status as EXECUTED</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SR_Status_Change_Restriction_on_Partner</fullName>
        <active>true</active>
        <description>This validation will ensure that the SR status cannot be changed to OPEN after it has been EXECUTED / CLOSED</description>
        <errorConditionFormula>AND (OR ($Profile.Name = &apos;Sukam Partner User&apos;, $Profile.Name = &apos;Dummy - Partner Profile&apos; ), ISCHANGED (Status__c), ISPICKVAL(Status__c, &apos;Open&apos;))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>The Service Request cannot be OPENED once it has been EXECUTED / CLOSED</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>To_date_has_to_be_after_From_date</fullName>
        <active>true</active>
        <description>&apos;To&apos; date cannot be before from date</description>
        <errorConditionFormula>To__c   &lt;   From__c</errorConditionFormula>
        <errorDisplayField>To__c</errorDisplayField>
        <errorMessage>The &apos;To&apos; date cannot be before the &apos;From&apos; date. Please check</errorMessage>
    </validationRules>
