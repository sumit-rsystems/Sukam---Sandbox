    <validationRules>
        <fullName>Component_and_Quantity1_Mandate</fullName>
        <active>true</active>
        <description>Where ever there is a Component chosen, it should have an associated quantity</description>
        <errorConditionFormula>AND( 
(Select_Component__c &lt;&gt; &quot;&quot; ), 
(From_Org__c &lt;&gt; &apos;019-SC-GGN&apos; ), 
( Type_of_Service_Request__c &lt;&gt; &quot;Warranty&quot;), 
ISNULL(Quantity__c),
 (Quantity__c &lt;&gt; 0))</errorConditionFormula>
        <errorDisplayField>Quantity__c</errorDisplayField>
        <errorMessage>Please enter a valid component and quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Component_and_Quantity2_Mandate</fullName>
        <active>true</active>
        <description>Where ever there is a Component chosen, it should have an associated quantity</description>
        <errorConditionFormula>(Select_Component_2__c  &lt;&gt; &quot;&quot; &amp;&amp;( Quantity_2__c   &lt;=   0 || ISNULL(Quantity_2__c))) ||
(Select_Component_2__c  = &quot;&quot; &amp;&amp;( Quantity_2__c   &lt;=   0 || NOT(ISNULL(Quantity_2__c)))) &amp;&amp;  From_Org__c &lt;&gt; &apos;019-SC-GGN&apos;</errorConditionFormula>
        <errorDisplayField>Quantity_2__c</errorDisplayField>
        <errorMessage>Please enter a valid component and quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Component_and_Quantity3_Mandate</fullName>
        <active>true</active>
        <description>Where ever there is a Component chosen, it should have an associated quantity</description>
        <errorConditionFormula>(Select_Component_3__c &lt;&gt; &quot;&quot; &amp;&amp;( Quantity_3__c &lt;= 0 || ISNULL(Quantity_3__c))) || 
(Select_Component_3__c = &quot;&quot; &amp;&amp;( Quantity_3__c &lt;= 0 || NOT(ISNULL(Quantity_3__c)))) &amp;&amp;  From_Org__c &lt;&gt; &apos;019-SC-GGN&apos;</errorConditionFormula>
        <errorDisplayField>Quantity_3__c</errorDisplayField>
        <errorMessage>Please enter a valid component and quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Component_and_Quantity4_Mandate</fullName>
        <active>true</active>
        <description>Where ever there is a Component chosen, it should have an associated quantity</description>
        <errorConditionFormula>(Select_Component_4__c &lt;&gt; &quot;&quot; &amp;&amp;( Quantity_4__c &lt;= 0 || ISNULL(Quantity_4__c))) || 
(Select_Component_4__c = &quot;&quot; &amp;&amp;( Quantity_4__c &lt;= 0 || NOT(ISNULL(Quantity_4__c)))) &amp;&amp;  From_Org__c &lt;&gt; &apos;019-SC-GGN&apos;</errorConditionFormula>
        <errorDisplayField>Quantity_4__c</errorDisplayField>
        <errorMessage>Please enter a valid component and quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Defective_Item_returned_Blank_Rule</fullName>
        <active>false</active>
        <errorConditionFormula>OR( IF( Defective_Item_is_different__c = True, 
ISBLANK( Defective_Item_Returned__c ) , 
False) , 
IF(Defective_Item_is_different__c = False , 
NOT( ISBLANK (Defective_Item_Returned__c) ) , 
false) )</errorConditionFormula>
        <errorDisplayField>Defective_Item_is_different__c</errorDisplayField>
        <errorMessage>If defective item is different then defective item returned can not be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Defective_Quantity_Validation</fullName>
        <active>true</active>
        <description>Defective Quantity Validation</description>
        <errorConditionFormula>AND
( ISPICKVAL(Component_Status__c , &apos;Defective&apos;), 
 $RecordType.Name = &apos;Stock Inward&apos;,
OR
(
 ISBLANK( Received_Quantity1__c ) = False, 
ISBLANK( Received_Quantity2__c )= False,
ISBLANK( Received_Quantity3__c )= False,
ISBLANK( Received_Quantity4__c )= False
))</errorConditionFormula>
        <errorMessage>Usable Quantity cannot be received when Component status is Defective.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Defective_Quantity_can_be_entered_once</fullName>
        <active>true</active>
        <description>In case of Warranty, Defective Quantity Value can be entered only once</description>
        <errorConditionFormula>AND(
ISCHANGED(  Defective_Quantity_Returned__c ), 
NOT(ISNULL (Defective_Quantity_Returned__c)))</errorConditionFormula>
        <errorDisplayField>Defective_Quantity_Returned__c</errorDisplayField>
        <errorMessage>In case of Warranty, Defective Quantity Value can be entered only once</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dispatched_Through_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
         AND( RecordTypeId  = &quot;01290000000NxCl&quot;,
                    ISPICKVAL( Type_of_Transfer__c , &quot;Stock Transfer&quot;) ) , 
                   
                AND( ISBLANK(  TEXT(Dispatched_Through__c) ) ,
                                  ISBLANK(  TEXT( Road_Permit__c )) ) , 
                                
                       False)</errorConditionFormula>
        <errorDisplayField>Dispatched_Through__c</errorDisplayField>
        <errorMessage>In case of Stock Transfer Dispatched Through &amp; Road Permit ban not be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>For_Service_Center_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT( ISBLANK( For_Service_Center__c ) ) ,
 Transferred_to_Stock_Point__c  &lt;&gt;  For_Service_Center__c ,
  $Profile.Name  &lt;&gt; &apos;System Administrator&apos;,
 ISPICKVAL(Stock_Transfer_Status__c , &apos;In Transit&apos;) ,
 RecordTypeId  = &apos;01290000000NxCl&apos;
)</errorConditionFormula>
        <errorMessage>Transferred to Stock Point should be equal to For Service Center</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>In_Out_should_be_equal_in_warranty</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
( RecordType.DeveloperName  = &quot;Components Used in Service Request&quot;) ,
(Defective_Quantity_Returned__c    &lt;&gt;    Quantity__c     ),
OR(
(  $ObjectType.Service_Request__c.Fields.Warranty_Status__c  = &quot;Yes&quot; )))</errorConditionFormula>
        <errorMessage>In_Warranty_Qty._1_&amp;_Defective_should_be</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Sale_Price</fullName>
        <active>true</active>
        <description>Sale Price is mandatory for all Service Requests that are &quot;Sale of Component&quot; Type</description>
        <errorConditionFormula>AND(Type_of_Service_Request__c = &quot;Sale of Component&quot;, Sale_Price_Unit__c = 0)</errorConditionFormula>
        <errorMessage>Please put in a Sale Price for Item Selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Recieving_Stock_Point</fullName>
        <active>true</active>
        <description>In Case of &quot;Returned by Customer&quot; Inward Movement, there is no &quot;Received from Stock Point&quot;</description>
        <errorConditionFormula>AND( 
ISPICKVAL(Type_of_Receipt__c, &quot;Returned by Customer&quot;), 
(Received_from_Stock_Point__c &lt;&gt; &quot;&quot;))</errorConditionFormula>
        <errorMessage>In Case of &quot;Returned by Customer&quot; Inward Movement, there is no &quot;Received from Stock Point&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Recieving_Stock_Points</fullName>
        <active>true</active>
        <description>In Case of &quot;Purchase&quot; Inward Movement, there is no &quot;Received from Stock Point&quot;</description>
        <errorConditionFormula>AND(
ISPICKVAL(Type_of_Receipt__c, &quot;Purchase&quot;),   
(Received_from_Stock_Point__c  &lt;&gt;  &quot;&quot;))</errorConditionFormula>
        <errorMessage>In Case of &quot;Purchase&quot; Inward Movement, there is no &quot;Received from Stock Point&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Sale_Price_for_FOC_Component</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Type_of_Service_Request__c = &quot;FOC&quot;, Sale_Price_Unit__c  &lt;&gt; 0)</errorConditionFormula>
        <errorMessage>Kindly check that the Component Sale Price is nil in case of FOC and Warranty Cases</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Sale_Price_for_Warranty_Component</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Type_of_Service_Request__c = &quot;Warranty&quot;, Sale_Price_Unit__c &lt;&gt; 0)</errorConditionFormula>
        <errorMessage>Kindly check that the Component Sale Price is nil in case of FOC and Warranty Cases</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Once_Created_Can_not_be_changed</fullName>
        <active>true</active>
        <errorConditionFormula>AND( $Profile.Name  &lt;&gt; &apos;System Administrator&apos;, 
                    $Profile.Name &lt;&gt; &apos;sukam System Administrator&apos; ,
 Is_Created__c  = True ,
 RecordTypeId  = &apos;01290000000NxCi&apos;)</errorConditionFormula>
        <errorDisplayField>Basic__c</errorDisplayField>
        <errorMessage>Can Not Change the record Once it has been Created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Purchase_Rate_1_is_mandatory</fullName>
        <active>true</active>
        <description>For Inward Movement &quot;Purchase Rate&quot; is mandatory for Type of Reciept being PURCHASE</description>
        <errorConditionFormula>AND(
ISPICKVAL(  Type_of_Receipt__c  , &quot;Purchase&quot;) , 
(Select_Component__c   &lt;&gt;   &quot;&quot; ),
OR(
(Purchase_Rate_Unit_1__c &lt;= 0),
ISNULL(Purchase_Rate_Unit_1__c )))</errorConditionFormula>
        <errorDisplayField>Purchase_Rate_Unit_1__c</errorDisplayField>
        <errorMessage>Please put in the Purchase Rate / Unit 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Purchase_Rate_2_is_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(  Type_of_Receipt__c  , &quot;Purchase&quot;) , 
( Select_Component_2__c  &lt;&gt;   &quot;&quot; ),
OR(
( Purchase_Rate_Unit_2__c  &lt;= 0),
ISNULL( Purchase_Rate_Unit_2__c  )))</errorConditionFormula>
        <errorDisplayField>Purchase_Rate_Unit_2__c</errorDisplayField>
        <errorMessage>Please put in the Purchase Rate / Unit 2</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Purchase_Rate_3_is_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL( Type_of_Receipt__c , &quot;Purchase&quot;) , 
(  Select_Component_3__c  &lt;&gt; &quot;&quot; ), 
OR( 
(  Purchase_Rate_Unit_3__c &lt;= 0), 
ISNULL(  Purchase_Rate_Unit_3__c  )))</errorConditionFormula>
        <errorDisplayField>Purchase_Rate_Unit_3__c</errorDisplayField>
        <errorMessage>Please put in the Purchase Rate / Unit 3</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Purchase_Rate_4_is_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL( Type_of_Receipt__c , &quot;Purchase&quot;) , 
(  Select_Component_4__c  &lt;&gt; &quot;&quot; ), 
OR( 
(  Purchase_Rate_Unit_4__c &lt;= 0), 
ISNULL(  Purchase_Rate_Unit_4__c  )))</errorConditionFormula>
        <errorDisplayField>Purchase_Rate_Unit_4__c</errorDisplayField>
        <errorMessage>Please put in the Purchase Rate / Unit 4</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Qty_Used_is_equal_Qty_Returned_Defective</fullName>
        <active>false</active>
        <description>This is a validation to ensure that in case (Type of Service Request = FOC / Warranty) of &quot;Component Used in Service Request&quot;, the quantity of item issued is equal or more than the defective item returned</description>
        <errorConditionFormula>AND( 
(RecordTypeId = &quot;01290000000NxCi&quot;) , 
(Defective_Quantity_Returned__c &gt; Quantity__c ), 
OR( 
( Type_of_Service_Request__c = &quot;Warranty&quot; ) , 
( Type_of_Service_Request__c = &quot;FOC&quot; ) ) 
)</errorConditionFormula>
        <errorDisplayField>Defective_Quantity_Returned__c</errorDisplayField>
        <errorMessage>Please check: Defective Quantity Returned cannot be more than Quantity 1(Quantity Issued).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quantity_Cannot_be_more_than_9999</fullName>
        <active>true</active>
        <description>The value in the fields quantity 1, quantity 2, quantity 3, quantity 4 cannot be more than 9999</description>
        <errorConditionFormula>OR (  Quantity__c  &gt; 9999,  Quantity_2__c &gt; 9999, Quantity_3__c &gt; 9999, Quantity_4__c &gt; 9999 )</errorConditionFormula>
        <errorDisplayField>Basic__c</errorDisplayField>
        <errorMessage>The Quantity cannot be greater than 9999</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Road_permit_can_not_be_blank</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISPICKVAL( Road_Permit__c , &quot;Yes&quot;) , 
                   ISBLANK( Road_Permit_No__c ) , 
                              False)</errorConditionFormula>
        <errorDisplayField>Road_Permit_No__c</errorDisplayField>
        <errorMessage>Fill Road Permit No. if road permit is YES.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sale_Price_Mandatory</fullName>
        <active>false</active>
        <description>In case of Component Used in Service Request, If Type of Service Request is &quot;Sale of Component&quot; then Sale Price / Unit 1 is mandatory</description>
        <errorConditionFormula>AND(
(RecordTypeId = &quot;01290000000NxCi&quot;),
(Type_of_Service_Request__c = &quot;Sale of Component&quot;),
(Select_Component__c   &lt;&gt;   &quot;&quot; ),
OR(
( Sale_Price_Unit__c &lt;= 0),
ISNULL( Sale_Price_Unit__c )))</errorConditionFormula>
        <errorDisplayField>Sale_Price_Unit__c</errorDisplayField>
        <errorMessage>Please put in value of &quot;Sale Price / Unit 1&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Service_Engineer_has_resigned</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordTypeId = &quot;01290000000NxCl&quot;, 
 OR( Transferred_to_Stock_Point__r.Resign__c = True,
                 Transferred_to_Stock_Point__r.Active__c  = False) ,
ISPICKVAL(Type_of_Transfer__c, &quot;Transferred to Service Engineer&quot;) )</errorConditionFormula>
        <errorMessage>Can not move stock to this Engineer as he has resigned.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Service_Request_No_is_manadatory</fullName>
        <active>true</active>
        <description>This is a validation to prevent user from creating any &quot;Component Used in Service Request&quot; transaction from &quot;Component Movement Table&quot;</description>
        <errorConditionFormula>AND(
(RecordTypeId = &quot;0122000000094Ca&quot;) ,
(Service_Request__c = &quot;&quot;))</errorConditionFormula>
        <errorMessage>Please go through the related list option to create this entry</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TRC_1_Branch_and_Defective_Status</fullName>
        <active>true</active>
        <description>can not send Defective items to TRC 1 Branch</description>
        <errorConditionFormula>AND( ISPICKVAL(Component_Status__c , &apos;Defective&apos;) ,
 Transferred_to_Stock_Point__r.Service_Center_Location_Code__c  = &apos;1111-TRC Branch 1&apos;)</errorConditionFormula>
        <errorMessage>can not send Defective items to TRC 1 Branch</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transfer_to_service_Engineer_Rule</fullName>
        <active>false</active>
        <errorConditionFormula>AND( RecordTypeId  = &apos;01290000000NxCl&apos;,
 ISPICKVAL( Type_of_Transfer__c , &quot;Transferred To Service Engineer&quot;) , 
  Transferred_to_Stock_Point__r.Service_Center_Location_Code__c  &lt;&gt;  Transferred_to_Stock_Point__r.Related_to_Service_Center__r.Service_Center_Location_Code__c )</errorConditionFormula>
        <errorMessage>Service Center can transfer stocks to his related service Engineer Only not to other Service Center&apos;s Engineers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transfer_to_service_Engineer_rule_1</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( Type_of_Transfer__c , &apos;Stock Transfer&apos;) ,
 Transferred_to_Stock_Point__r.RecordTypeId  = &quot;01290000000NwDa&quot;)</errorConditionFormula>
        <errorDisplayField>Type_of_Transfer__c</errorDisplayField>
        <errorMessage>Type of transfer should be Transferred to Service Eng. while transferring Stock to Service Engineer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transferred_Qty_equals_Received_Qty</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND
( ISPICKVAL(Component_Status__c , &apos;Usable&apos;), 
 OR(( $RecordType.Name = &apos;Stock Inward&apos;),( $RecordType.Name  = &apos;Stock Received&apos;) ) ), 
OR(
(Quantity__c &lt;&gt;Received_Quantity1__c) , 
AND (( NOT( ISNULL(Quantity__c) ) ), ISNULL(Received_Quantity1__c)),
(Quantity_2__c &lt;&gt;Received_Quantity2__c) , 
AND (( NOT( ISNULL(Quantity_2__c) ) ), ISNULL(Received_Quantity2__c)),
(Quantity_3__c &lt;&gt; Received_Quantity3__c) ,
AND (( NOT( ISNULL(Quantity_3__c ) ) ), ISNULL(Received_Quantity3__c)),
(Quantity_4__c &lt;&gt; Received_Quantity4__c) ,
AND (( NOT( ISNULL(Quantity_4__c ) ) ), ISNULL(Received_Quantity4__c))
   ),
false)</errorConditionFormula>
        <errorDisplayField>Basic__c</errorDisplayField>
        <errorMessage>Sum of Transferred Quantity should be equal to Received Quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transferred_Qty_equals_to_Defective_Qty</fullName>
        <active>true</active>
        <description>Transferred Quantity = Defective Quantity</description>
        <errorConditionFormula>IF(AND 
( ISPICKVAL(Component_Status__c , &apos;Defective&apos;), 
OR(( $RecordType.Name = &apos;Stock Inward&apos;),( $RecordType.Name = &apos;Stock Received&apos;) ) ), 
OR( 
(Quantity__c &lt;&gt;Defective_Quantity1__c) , 
AND (( NOT( ISNULL(Quantity__c) ) ), ISNULL(Defective_Quantity1__c)), 
(Quantity_2__c &lt;&gt;Defective_Quantity2__c) , 
AND (( NOT( ISNULL(Quantity_2__c) ) ), ISNULL(Defective_Quantity2__c)), 
(Quantity_3__c &lt;&gt; Defective_Quantity3__c) , 
AND (( NOT( ISNULL(Quantity_3__c ) ) ), ISNULL(Defective_Quantity3__c)), 
(Quantity_4__c &lt;&gt; Defective_Quantity4__c) , 
AND (( NOT( ISNULL(Quantity_4__c ) ) ), ISNULL(Defective_Quantity4__c)) 
) ,false)</errorConditionFormula>
        <errorDisplayField>Basic__c</errorDisplayField>
        <errorMessage>Transferred Quantity should be equal to Defective Quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Defective_Short_Quantity</fullName>
        <active>true</active>
        <description>Validate_Defective_Short_Quantity</description>
        <errorConditionFormula>$RecordType.Name = &apos;Stock Inward&apos; &amp;&amp; Short_Receipt_Quantity1__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Received_Quantity1__c</errorDisplayField>
        <errorMessage>Enter Appropriate Quantities</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Owner_Id</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL(Type_of_Receipt__c,&apos;Stock Returned by Service Engineer&apos; ) || ISPICKVAL( Type_of_Transfer__c ,&apos;Transferred to Service Engineer&apos;) ) &amp;&amp; (Stock_Point_Owner_Id__c &lt;&gt; OwnerId) &amp;&amp; Stock_Point_Type__c = &apos;Service Engineer&apos;</errorConditionFormula>
        <errorDisplayField>Basic__c</errorDisplayField>
        <errorMessage>The Service Engineer Choosen doesn&apos;t belong to the Service Center. Choose an appropriate Service Engineer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_SC_Stock_Point_Type</fullName>
        <active>false</active>
        <description>Validate_SC_Stock_Point_Type</description>
        <errorConditionFormula>ISPICKVAL( Type_of_Transfer__c ,&apos;Stock Transfer&apos; ) &amp;&amp; Stock_Point_Type__c &lt;&gt; &apos;Service Center&apos; &amp;&amp; From_Org__c &lt;&gt;&apos;019-SC-GGN&apos;</errorConditionFormula>
        <errorMessage>Please choose a Service Center for this transaction.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_SE_Stock_Point_Type</fullName>
        <active>true</active>
        <description>Validate_SE_Stock_Point_Type</description>
        <errorConditionFormula>((ISPICKVAL(Type_of_Receipt__c,&apos;Stock Returned by Service Engineer&apos; ) || ISPICKVAL( Type_of_Transfer__c ,&apos;Transferred to Service Engineer&apos;) ) &amp;&amp; Stock_Point_Type__c &lt;&gt; &apos;Service Engineer&apos; )</errorConditionFormula>
        <errorMessage>Please choose your own Service Engineer for this transaction.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>check_null</fullName>
        <active>false</active>
        <errorConditionFormula>AND (( NOT( ISNULL(Quantity__c) ) ), ISNULL(Received_Quantity1__c))</errorConditionFormula>
        <errorMessage>check</errorMessage>
    </validationRules>
