        <fullName>Purchaser_TIN_No_is_mandatory</fullName>
        <active>true</active>
        <description>Purchaser TIN No is mandatory in case of Registered Purchaser</description>
        <errorConditionFormula>AND(
ISPICKVAL( Purchaser_Type__c  , &quot;Registered&quot;),
(Purchaser_TIN_No__c = &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Purchaser_TIN_No__c</errorDisplayField>
        <errorMessage>Purchaser TIN No is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TCS_cant_be_negative</fullName>
        <active>true</active>
        <description>TCS % Can&apos;t be negative</description>
        <errorConditionFormula>TCS__c &lt; 0</errorConditionFormula>
        <errorDisplayField>TCS__c</errorDisplayField>
        <errorMessage>TCS % can&apos;t be negative.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_State_for_InterState</fullName>
