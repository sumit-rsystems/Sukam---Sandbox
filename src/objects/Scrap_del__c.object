        <errorConditionFormula>ISPICKVAL(Sale_Location_Type__c , &apos;Interstate&apos;) &amp;&amp;  ISPICKVAL(Purchaser_Billing_State__c ,&apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Purchaser_Billing_State__c</errorDisplayField>
        <errorMessage>Please choose the Purchaser&apos;s Billing State</errorMessage>
    </validationRules>
