    <fields>
        <fullName>Dispatch_Status_Indicator__c</fullName>
        <externalId>false</externalId>
        <formula>IF ( Total_Number_of_Closed_Order_Line_Items__c = Total_Order_Line_Items__c , 1, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Dispatch Status Indicator</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Dispatch_Status__c</fullName>
        <externalId>false</externalId>
        <label>Dispatch Status</label>
        <picklist>
            <picklistValues>
                <fullName>Pending</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>In-Transit</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Part Shipment Received</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Closed</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Item_Code__c</fullName>
        <externalId>false</externalId>
        <label>Item Code</label>
        <length>15</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Model__c</fullName>
        <description>This wil contain the product model used for Su-Kam PRM</description>
        <externalId>false</externalId>
        <label>Model</label>
        <length>50</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Net_Price__c</fullName>
        <description>This will calculate the net price for every order line-item based on the price-book value and the quantity ordered</description>
        <externalId>false</externalId>
        <formula>Price_Rate__r.Distributor__c *  Order_Quantity__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Net Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Order_Count__c</fullName>
        <externalId>false</externalId>
        <label>Order Count</label>
        <precision>6</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Order_Date_Time__c</fullName>
        <externalId>false</externalId>
        <label>Order Date / Time</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Order_Date__c</fullName>
        <externalId>false</externalId>
        <label>Order Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Order_No__c</fullName>
        <displayFormat>O-{0000}</displayFormat>
        <externalId>false</externalId>
        <label>Order No</label>
        <trackTrending>false</trackTrending>
        <type>AutoNumber</type>
    </fields>
    <fields>
        <fullName>Order_Quantity__c</fullName>
        <externalId>false</externalId>
        <label>Order Quantity</label>
        <precision>5</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Order_Status__c</fullName>
        <externalId>false</externalId>
        <label>Order Status</label>
        <picklist>
            <picklistValues>
                <fullName>Draft</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Submitted</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>On-hold</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Price_Rate__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>This will pull the price rate from the price-book</description>
        <externalId>false</externalId>
        <label>Price Rate</label>
        <referenceTo>Price_Book__c</referenceTo>
        <relationshipLabel>Orders</relationshipLabel>
        <relationshipName>Orders</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Product_Category__c</fullName>
        <description>This will contain the product category which will be ordered for.</description>
        <externalId>false</externalId>
        <label>Product Category</label>
        <length>50</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Remarks__c</fullName>
        <externalId>false</externalId>
        <label>Remarks</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(Order_Status__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Total_Number_of_Closed_Order_Line_Items__c</fullName>
        <description>This will calculate the total number of order line items that have been closed i.e order line items where the dispatched qty equals or is greater than approved qty.</description>
        <externalId>false</externalId>
        <label>Total Number of Closed Order Line Items</label>
        <summarizedField>Order_Line_Item__c.Order_Line_Item_Despatch__c</summarizedField>
        <summaryForeignKey>Order_Line_Item__c.Order__c</summaryForeignKey>
        <summaryOperation>sum</summaryOperation>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>Total_Order_Line_Items__c</fullName>
        <description>This contains a number equal to the total number of order line items</description>
        <externalId>false</externalId>
        <label>Total Order Line Items</label>
        <summaryForeignKey>Order_Line_Item__c.Order__c</summaryForeignKey>
        <summaryOperation>count</summaryOperation>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>submitted_time__c</fullName>
        <externalId>false</externalId>
        <label>submitted time</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <label>Order</label>
    <listViews>
