            <value>Submitted</value>
        </filters>
        <label>My Submitted Orders</label>
        <sharedTo>
            <group>AllPartnerUSer</group>
        </sharedTo>
    </listViews>
    <listViews>
        <fullName>On_Hold_Orders</fullName>
