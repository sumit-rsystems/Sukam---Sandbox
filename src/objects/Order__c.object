        <filterScope>Everything</filterScope>
        <filters>
            <field>Order_Status__c</field>
            <operation>equals</operation>
            <value>On-hold</value>
        </filters>
        <label>&quot;On-Hold&quot; Orders</label>
        <sharedTo>
            <group>Sales</group>
        </sharedTo>
    </listViews>
